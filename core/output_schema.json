          }
        }
      },
      "additionalProperties": false,
      "required": [
        "SpaceNames"
      ]
    },
    {
      "type": "object",
      "properties": {
        "KeyNames": {
          "type": "array",
          "items": {
            "type": "string"
          }
        }
      },
      "additionalProperties": false,
      "required": [
        "KeyNames"
      ]
    },
    {
      "type": "object",
      "properties": {
        "GetValue": {
          "type": "array",
          "items": {
            "type": "array",
            "maxItems": 3,
            "minItems": 3,
            "prefixItems": [
              {
                "$ref": "#/$defs/SpaceTimeId"
              },
              {
                "type": "array",
                "items": {
                  "$ref": "#/$defs/Point"
                },
                "maxItems": 8,
                "minItems": 8
              },
              {
                "$ref": "#/$defs/ValueEntry"
              }
