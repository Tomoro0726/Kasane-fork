          "type": "object",
          "properties": {
            "Single": {
              "type": "integer",
              "format": "uint64",
              "minimum": 0
            }
          },
          "additionalProperties": false,
          "required": [
            "Single"
          ]
        },
        {
          "description": "An Any value, indicating it applies to all possible values in this dimension (e.g., -).",
          "type": "string",
          "const": "Any"
        }
